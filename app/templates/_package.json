{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "My module",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %><% if (libDir) { %>,
    "lib/"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.0.0"
  <% } %>},
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4"
  },
  "options": {
    "mocha": "--compilers js:babel-register --require babel-polyfill ./**/__tests__/**/*.spec.js"
  },
  "scripts": {
    "postinstall": "npm run build",
    "clean": "rimraf lib dist .nyc_output",
    "build": "npm run clean && babel src -d lib --ignore __tests__",
    "testonly": "NODE_ENV=test mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test:cov": "nyc npm test",
    "lint": "eslint ."
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
